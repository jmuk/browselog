{
  "name": "browselog",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Logs every brwosing and store the log to GoogleSpreadsheet.",
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "permissions": [
    "tabs", "background", "https://accounts.google.com/o/oauth2/token", "https://spreadsheets.google.com/*"
  ]
}
